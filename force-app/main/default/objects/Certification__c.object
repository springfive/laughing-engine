        <description>Does this certification counts towards the &quot;Practice Growth&quot; indicator?</description>
        <externalId>false</externalId>
        <inlineHelpText>Does this certification counts towards the &quot;Practice Growth&quot; indicator?</inlineHelpText>
        <label>Is Practice Growth?</label>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Checkbox</type>
    </fields>
    <fields>
        <fullName>MaxPoints__c</fullName>
        <description>Gives the maximum number of points that a certification level will give at the company level.</description>
        <externalId>false</externalId>
        <formula>CASE(TEXT(CredentialLevel__c), &quot;L1&quot;, 30, &quot;L2&quot;, 40, &quot;L3&quot;, 80, &quot;L4&quot;, 50, 0)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <inlineHelpText>Gives the maximum number of points that a certification level will give at the company level.</inlineHelpText>
        <label>Max Points</label>
        <precision>18</precision>
        <required>false</required>
        <scale>0</scale>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Number</type>
        <unique>false</unique>
    </fields>
