    <fields>
        <fullName>manDayBookedLayout__c</fullName>
        <description>Number of Man/day that have been booked for aSupport Request. If the checkbox &quot;Free&quot; is True, it displays &quot;FREE&quot; otherwise it displays the &quot;S5 - Man/day booked&quot; field value. 
Field to be displayed on the Customer Community Layout.</description>
        <externalId>false</externalId>
        <formula>IF (  Free__c = TRUE,
0,

IF( ISPICKVAL(Status,&quot;New&quot;) || ISPICKVAL(Status,&quot;TO_BE_QUALIFIED&quot;) || ISPICKVAL(Status,&quot;TO_BE_QUALIFIED_MORE_INFO&quot;),NULL,

IF( ISPICKVAL(Status,&quot;QUALIFIED_TO_BE_APPROVED&quot;) || ISPICKVAL(Status,&quot;TO_BE_IMPLEMENTED&quot;) || ISPICKVAL(Status,&quot;IMPLEMENTED&quot;) || ISPICKVAL(Status,&quot;TESTED_Ok&quot;) || ISPICKVAL(Status,&quot;Tested KO&quot;) || ISPICKVAL(Status,&quot;Delivered&quot;), manDayBooked__c,

IF ( ISPICKVAL(Status,&quot;Rejected&quot;) || ISPICKVAL(Status,&quot;Cancelled&quot;) || ISPICKVAL(Status,&quot;Postponed&quot;), manDayConsumed__c,
null)
)))</formula>
        <inlineHelpText>Number of Man/day that have been booked for this Support Request.</inlineHelpText>
        <label>Estimated workload (Man-days)</label>
        <precision>18</precision>
        <required>false</required>
        <scale>2</scale>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Number</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>manDayBooked__c</fullName>
        <description>The evaluated number man / day needed to handle the Support Request from start to finish. Not displayed on the customer layout.</description>
        <externalId>false</externalId>
        <label>S5 - Estimated workload (Man-days)</label>
        <precision>18</precision>
        <required>false</required>
        <scale>2</scale>
        <trackFeedHistory>false</trackFeedHistory>
        <trackHistory>true</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Number</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>manDayConsumedLayout__c</fullName>
        <description>Number of Man/day that have been consumed for a Support Request. If the checkbox &quot;Free&quot; is True, it displays &quot;FREE&quot; otherwise it displays the &quot;S5 - Man/day Consumed&quot; field value. 
Field to be displayed on the Customer Community Layout.</description>
        <externalId>false</externalId>
        <formula>IF (  Free__c = TRUE,
IMAGE(&quot;https://springfivedev--dev--c.cs84.visual.force.com/resource/1535536629000/FreeStamp?isdtp=p1&quot;, &quot;FREE&quot;, 20,80),

CASE( Status ,
&quot;New&quot;, &quot;N/A&quot;,
&quot;TO_BE_QUALIFIED&quot;, &quot;N/A&quot;,
&quot;TO_BE_QUALIFIED_MORE_INFO&quot;, &quot;N/A&quot;,
&quot;QUALIFIED_TO_BE_APPROVED&quot;, &quot;N/A&quot;,

&quot;TO_BE_IMPLEMENTED&quot;,  TEXT(manDayConsumed__c),
&quot;IMPLEMENTED&quot;, TEXT(manDayConsumed__c),
&quot;TESTED_Ok&quot;, TEXT(manDayConsumed__c ),
&quot;Tested KO&quot;, TEXT(manDayConsumed__c),
&quot;Delivered&quot;, TEXT(manDayConsumed__c),

&quot;Rejected&quot;, TEXT(manDayConsumed__c ),
&quot;Cancelled&quot;, TEXT(manDayConsumed__c ),
&quot;Postponed&quot;, &quot;N/A&quot;,

null)
)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <inlineHelpText>Number of Man/day that have been consumed for this Support Request.</inlineHelpText>
        <label>Number of Man-days Consumed</label>
        <required>false</required>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
    </fields>
