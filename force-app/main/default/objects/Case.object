    <validationRules>
        <fullName>CAS_VR01_ForbiddenStatusesForCustomer</fullName>
        <active>true</active>
        <errorConditionFormula>( ISCHANGED(Status)  ||  ISNEW() ) &amp;&amp;
($Profile.Name = &apos;TMA Customer&apos;) &amp;&amp; 
NOT($User.BypassVR__c) &amp;&amp;
OR(
ISPICKVAL(Status, &apos;New&apos;) &amp;&amp; NOT(ISPICKVAL(PRIORVALUE(Status),&apos;New&apos;)),
ISPICKVAL(Status, &apos;TO_BE_QUALIFIED&apos;) &amp;&amp; NOT(ISPICKVAL(PRIORVALUE(Status),&apos;QUALIFIED_TO_BE_APPROVED&apos;)),
ISPICKVAL(Status,&apos;TO_BE_QUALIFIED_MORE_INFO&apos;),
ISPICKVAL(Status,&apos;QUALIFIED_TO_BE_APPROVED&apos;),
ISPICKVAL(Status,&apos;TO_BE_IMPLEMENTED&apos;) &amp;&amp; NOT(ISPICKVAL(PRIORVALUE(Status),&apos;QUALIFIED_TO_BE_APPROVED&apos;)),
ISPICKVAL(Status,&apos;IMPLEMENTED&apos;),
ISPICKVAL(Status,&apos;TESTED_Ok&apos;) &amp;&amp; NOT(ISPICKVAL(PRIORVALUE(Status),&apos;IMPLEMENTED&apos;)),
ISPICKVAL(Status,&apos;Tested KO&apos;) &amp;&amp; NOT(ISPICKVAL(PRIORVALUE(Status),&apos;IMPLEMENTED&apos;)),
ISPICKVAL(Status,&apos;Delivered&apos;),
ISPICKVAL(Status,&apos;Rejected&apos;),
ISPICKVAL(Status,&apos;Postponed&apos;)
)</errorConditionFormula>
        <errorDisplayField>Status</errorDisplayField>
        <errorMessage>Sorry, this Status value can only be picked by the SpringFive Support Team.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>CAS_VR02_InsufficientManDays</fullName>
        <active>true</active>
        <description>The system must automatically prevent the user from creating a support request if he has not enough remaining days.</description>
        <errorConditionFormula>AND(
(ISNEW() || ISCHANGED(Status) || ISCHANGED( manDayBooked__c )),
 NOT($User.BypassVR__c),
 NOT( Free__c ),
 ISPICKVAL(SupportProject__r.Payment_Terms__c, &quot;Pre-Paid&quot;),   
 (SupportProject__r.manDayPrepaidProject__c  -  (SupportProject__r.manDayConsumed__c) )   &lt; SupportProject__r.MinimumBillableDay__c
)</errorConditionFormula>
        <errorMessage>The Remaining Days are not sufficient to go through the Support Request. Please contact a SpringFive agent in order to recredit your account.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>CAS_VR03_NoWorkloadModifAfterApproval</fullName>
        <active>true</active>
        <errorConditionFormula>NOT($User.BypassVR__c) &amp;&amp;
OR(
ISPICKVAL(Status,&quot;TO_BE_IMPLEMENTED&quot;),
ISPICKVAL(Status,&quot;IMPLEMENTED&quot;),
ISPICKVAL(Status,&quot;TESTED_Ok&quot;),
ISPICKVAL(Status,&quot;Tested KO&quot;),
ISPICKVAL(Status,&quot;Delivered&quot;),
ISPICKVAL(Status,&quot;Cancelled&quot;),
ISPICKVAL(Status,&quot;Postponed&quot;),
ISPICKVAL(Status,&quot;Rejected&quot;)) 
&amp;&amp; 
OR(  
PRIORVALUE( manDayBooked__c )  &lt;&gt; manDayBooked__c,
PRIORVALUE(  S5DesignOption__c  )  &lt;&gt; S5DesignOption__c,
PRIORVALUE(   DesignOption__c   )  &lt;&gt; DesignOption__c
)</errorConditionFormula>
        <errorMessage>The Estimation provided and the Design Options cannot be changed when the Support Request has the current Status.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>CAS_VR05_ProposalProvidedBlankEstimated</fullName>
        <active>true</active>
        <description>The system must autoamtically prevent a user from selecting the status &quot;Proposal Provided - To Be Approved&quot; if the estimated workload field is blank.</description>
        <errorConditionFormula>AND(
 NOT($User.BypassVR__c),
 ISCHANGED( Status ),
 ISPICKVAL( Status , &quot;QUALIFIED_TO_BE_APPROVED&quot;),
 NOT( Free__c ),
 ISBLANK( manDayBooked__c )
)</errorConditionFormula>
        <errorDisplayField>manDayBooked__c</errorDisplayField>
        <errorMessage>You must enter a value on the estimated workload field in order to provide the proposal.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>CSP_VR06_InsufficientManDaysToValidate</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
NOT($User.BypassVR__c), 
NOT( Free__c ),
ISPICKVAL(SupportProject__r.Payment_Terms__c, &quot;Pre-Paid&quot;), 
OR(
( (SupportProject__r.manDayPrepaidProject__c - SupportProject__r.manDayConsumed__c &lt;  0 ) &amp;&amp; ISPICKVAL(Status, &quot;TO_BE_IMPLEMENTED&quot;) ),
( (SupportProject__r.manDayPrepaidProject__c - SupportProject__r.manDayConsumed__c - manDayBooked__c &lt; 0 ) &amp;&amp; ISPICKVAL(Status, &quot;QUALIFIED_TO_BE_APPROVED&quot;) )
)
)</errorConditionFormula>
        <errorMessage>The remaining days are not sufficient to validate the estimation of this Support Request.</errorMessage>
    </validationRules>
</CustomObject>
