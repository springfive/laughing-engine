    <validationRules>
        <fullName>ACTIV_VR01_InterditDeModifierLeNomEtComp</fullName>
        <active>true</active>
        <description>Une fois crée on ne peut pas modifier le nom ou le compte de l’activité</description>
        <errorConditionFormula>/*Bypass*/
!$Setup.CS01_Bypass__c.Bypass_ValidationRule__c
&amp;&amp;
IF(ISBLANK($Setup.CS01_Bypass__c.Bypass_VRbyVR__c),TRUE,
!CONTAINS($Setup.CS01_Bypass__c.Bypass_VRbyVR__c,&apos;ACTIV_VR01&apos;))
&amp;&amp;
IF(ISBLANK($Setup.CS01_Bypass__c.Bypass_VRbyObject__c),TRUE,
!CONTAINS($Setup.CS01_Bypass__c.Bypass_VRbyObject__c,&apos;Activite_Soufflet__c&apos;))

/*Business Rule*/
&amp;&amp;
OR(ISCHANGED( Name ), ISCHANGED ( Compte__c ))
&amp;&amp;
Tech_Force_Name_Change__c = False</errorConditionFormula>
        <errorMessage>ACTIV_VR01 - Une fois crée vous ne pouvez pas modifier le nom ou le compte de l’activité. Le nom de l&apos;activité est déduit de la Division et de la Société du commercial ainsi que du type d&apos;activité lors de la création.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ACTIV_VR02_QualifReqtSiAdresseActiveInac</fullName>
        <active>true</active>
        <description>Pour Qualifier une activité en Reconquête il faut faire un check si il y a une Adresse Facturée au moins Active ou Inactive</description>
        <errorConditionFormula>/*Bypass*/
!$Setup.CS01_Bypass__c.Bypass_ValidationRule__c
&amp;&amp;
IF(ISBLANK($Setup.CS01_Bypass__c.Bypass_VRbyVR__c),TRUE,
!CONTAINS($Setup.CS01_Bypass__c.Bypass_VRbyVR__c,&apos;ACTIV_VR02&apos;))
&amp;&amp;
IF(ISBLANK($Setup.CS01_Bypass__c.Bypass_VRbyObject__c),TRUE,
!CONTAINS($Setup.CS01_Bypass__c.Bypass_VRbyObject__c,&apos;Activite_Soufflet__c&apos;))

/*Business Rule*/
&amp;&amp;
ISCHANGED( Cycle_de_qualification__c )
&amp;&amp;
ISPICKVAL(Cycle_de_qualification__c , &quot;65&quot;)
&amp;&amp;
TECH_Activite_PeutEtreEnReconquette__c = FALSE</errorConditionFormula>
        <errorDisplayField>Cycle_de_qualification__c</errorDisplayField>
        <errorMessage>ACTIV_VR02 - Pour Qualifier une activité en Reconquête il faut avoir au moins une Adresse de facturation Active ou Inactive</errorMessage>
    </validationRules>
